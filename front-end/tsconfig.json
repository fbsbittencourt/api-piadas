{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext", // Mude de "CommonJS" ou similar para "ESNext"
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "skipLibCheck": true,
    "strict": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable"]
  }
}
